URxvt*font: xft:JetBrains Mono:size=12
URxvt*boldFont: xft:JetBrains Mono:bold:size=12

URxvt*scrollBar: false

URxvt*termName: xterm-256color

URxvt.perl-ext-common: default,tabbed
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.new-button: false

URxvt.keysym.Control-Shift-t: perl:tabbed:new_tab
URxvt.keysym.Control-Shift-w: perl:tabbed:close_tab
URxvt.keysym.Control-Shift-Right: perl:tabbed:next_tab
URxvt.keysym.Control-Shift-Left: perl:tabbed:prev_tab
